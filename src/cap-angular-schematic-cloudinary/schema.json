{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsCloudinary",
  "title": "CAP Cloudinary Schema",
  "type": "object",
  "properties": {
    "angular_version": {
      "type": "number",
      "description": "Angular version: ",
      "x-prompt": "Angular version [2-5]: ",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "cloud_name": {
      "type": "string",
      "description": "Cloud name",
      "x-prompt": "Cloudinary cloud name: ",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    },
    "upload_preset": {
      "type": "string",
      "description": "Upload preset: ",
      "x-prompt": "Cloudinary upload preset: ",
      "$default": {
        "$source": "argv",
        "index": 2
      }
    },
    "private_cdn": {
      "type": "string",
      "description": "Private cdn",
      "x-prompt": "Cloudinary private cdn: ",
      "$default": {
        "$source": "argv",
        "index": 3
      }
    },
    "cname": {
      "type": "string",
      "description": "cname:",
      "x-prompt": "Cloudinary cname: ",
      "$default": {
        "$source": "argv",
        "index": 4
      }
    }
  },
  "required": [
    "angular_version",
    "cloud_name",
    "upload_preset",
    "private_cdn",
    "cname"
  ]
}